page{
  background: #efefef;
}
.box{
  width: 700rpx;
  background: #fff;
  border-radius: 30rpx;
  margin: 40rpx auto;
  padding: 60rpx 30rpx;
  box-sizing: border-box;
  overflow: hidden;
  font-size: 32rpx;
  height: 200rpx;
}
.box-l{
  float: left;
}
.box-r{
  float: right;
  color: #333;
  font-size: 28rpx;
  border: 1px solid #333;
  border-radius: 30rpx;
  height: 60rpx;
  line-height: 60rpx;
  width: 160rpx;
  text-align: center;
}
.box-l-to{
  font-size: 22rpx;
  color: #424242;
  margin-top: 20rpx;
}
.box-l-t{
  font-weight: bold;
  font-size: 30rpx;
}
.tishi{
  color:#424242;
  font-size: 22rpx;
  display: block;
  text-align: center;
}