<page>
  <view class='mine-nav'>
    <view class="mine-tab {{tab==0?'active':''}}" bindtap='navTab' data-id="0">全部</view>
    <view class="mine-tab {{tab==1?'active':''}}" bindtap='navTab' data-id="1">待付款</view>
    <view class="mine-tab {{tab==2?'active':''}}" bindtap='navTab' data-id="2">已完成</view>
