.list button{
  border-radius:0;
  background:none;
  text-align: left;
  padding: 0;
  height: 120rpx;
  color: #333;
  font-size: 30rpx;
  line-height: 120rpx;
  position: relative;
}
button::after{
border:none;
}
.tese{
  width: 700rpx;
  margin: 0 auto;
  margin-top: 170rpx;
}
.tese image{
  width: 100%;
  height: 110rpx;
}
.act{
  margin-top: 130rpx
}
.speed{
  font-size: 24rpx;
  color: #333;
  width: 580rpx;
  margin: 0 auto;
  padding-top: 50rpx;
  overflow: hidden;
}
.speed view{
  width: 25%;
  float: left;
  position: relative;
  
}
.speed view::before{
  content: '';
  width: 12rpx;
  height: 12rpx;
  background: #adadad;
  border-radius: 50%;
  position: absolute;
  top: -20rpx;
  left: 60rpx;
}
.speed view::after{
  content: '';
  width: 120rpx;
  height: 4rpx;
  background: #d7d7d7;
  position: absolute;
  top: -16rpx;
  left:-66rpx;
}
.speed view:first-child::after{
  display: none;
}
.speed view.completed::before{
  content: '';
  width: 30rpx;
  height:30rpx;
  background: url(http://img.chaojibuyers.com:9011/jiaxiao/completed.png) no-repeat;
  background-size: 30rpx;
  position: absolute;
  top: -30rpx;
  left: 50rpx;
}
.speed view.loading::after{
  content: '';
  width: 110rpx;
  height: 4rpx;
  background: url(http://img.chaojibuyers.com:9011/jiaxiao/gif.gif) no-repeat;
  position: absolute;
  top: -16rpx;
  left:-66rpx;
}
.speed view.loading::before{
  content: '';
  width: 30rpx;
  height:30rpx;
  background: url(http://img.chaojibuyers.com:9011/jiaxiao/loading.png) no-repeat;
  background-size: 30rpx;
  position: absolute;
  top: -30rpx;
  left: 50rpx;
}
.speed::after{
  content: '';
  clear: both;
}
