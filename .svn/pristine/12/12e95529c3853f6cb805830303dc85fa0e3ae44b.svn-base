.nav{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #333;
  overflow: hidden;
  border: 1px solid #e5e5e5;
  font-size: 32rpx;
  font-weight: bold;
}
.nav view{
  width: 50%;
  text-align: center;
  float: left;
}
.nav view.active{
  color: #0196fd;
  position: relative;
}
.nav view.active::after{
  content: "";
  width: 100rpx;
  height: 4rpx;
  position: absolute;
  background: #0195ff;
  bottom: 0;
  left:50%;
  margin-left: -50rpx;
}
.list{
  height: 214rpx;
  box-sizing: border-box;
}
.list-l{
  width: 94rpx;
  height: 214rpx;
  text-align: center;
  line-height: 214rpx;
  float: left
}
.list-l image{
  width: 38rpx;
  height: 44rpx;
  vertical-align: middle;
}
.list-r{
  width: 656rpx;
  padding: 50rpx 0;
  border-bottom: 1px solid #e5e5e5;
  float: left;
}
.list-r-img{
  width: 114rpx;
  height: 114rpx;
  float: left;
  border: 1px solid #ececec;
  border-radius: 20rpx;
  margin-right: 20rpx;
}
.list-r-img image{
  width: 114rpx;
  height: 114rpx;
  border-radius: 20rpx;
}
.l-top{
  width: 518rpx;
  float: left;
  font-size: 30rpx;
  font-weight: bold;
  overflow: hidden;
  padding-right: 40rpx;
  box-sizing: border-box;
}
.l-top-r{
  float: right;
  font-size: 24rpx;
  font-weight: normal;
  color: #656565;
}
.pingfen{
  color: #333;
  font-size: 24rpx;
  margin-bottom: 14rpx;
}
.pingfen image{
  width: 22rpx;
  height: 22rpx;
  vertical-align: middle;
  margin-right: 20rpx;
}
.price{
  font-size: 26rpx;
  color: #333;
}
.price1{
  color: #ff4b66;
  font-size: 28rpx;
  margin-right: 20rpx;
}
.price2{
  margin-right: 20rpx;
}